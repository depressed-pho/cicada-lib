{
  "name": "cicada-lib",
  "version": "4.0.0",
  "description": "Library that improves Minecraft Bedrock Scripting API",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "exports": {
    "./*.js": "./dist/*.js"
  },
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "gulp",
    "build": "gulp",
    "clean": "gulp clean",
    "test": "NODE_OPTIONS='--import=tsimp/import' TSIMP_DIAG=error mocha 'test/**/*.ts'",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/depressed-pho/cicada-lib.git"
  },
  "keywords": [
    "Minecraft",
    "Bedrock",
    "Scripting",
    "Addon"
  ],
  "author": "PHO",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/depressed-pho/cicada-lib/issues"
  },
  "homepage": "https://github.com/depressed-pho/cicada-lib#readme",
  "devDependencies": {
    "@protobuf-ts/runtime": "^2.8.2",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.7",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "merge2": "^1.4.1",
    "mocha": "^10.2.0",
    "patch-package": "^8.0.0",
    "tsimp": "^2.0.10",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@minecraft/common": "^1.2.0-rc.1.20.70-preview.22",
    "@minecraft/server": "^1.9.0-beta.1.20.60-stable",
    "@minecraft/server-ui": "^1.2.0-beta.1.20.60-stable"
  },
  "peerDependencies": {
    "@protobuf-ts/runtime": "^2.8.2"
  }
}
